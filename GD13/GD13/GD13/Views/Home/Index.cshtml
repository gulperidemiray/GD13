<!--profiles start -->



<section id="profiles" class="profiles">
    <div class="profiles-details">
        <div class="section-heading text-center">
            <h2>profiles</h2>
        </div>
        <div class="container">
            <div class="profiles-content">
                <div class="row">

                    @{
                        foreach (var item in Model.ProfilListe)
                        {
                            <div class="col-sm-3">
                                <div class="single-profile">
                                    <div class="profile-txt" style="width: 24px; height: auto;">
                                        <a href="@item.profile_url"><img src="@item.profile_icon_url" alt="LinkedIn Icon"></a>
                                        <div class="profile-icon-name">@item.profile_name</div>
                                    </div>
                                    <div class="single-profile-overlay">
                                        <div class="profile-txt" style="width: 24px; height: auto;">
                                            <a href="@item.profile_url"><img src="@item.profile_icon_url" alt="LinkedIn Icon"></a>
                                            <div class="profile-icon-name">@item.profile_name</div>
                                            @{
                                                string ProfilSil = "/Profil/ProfilSil/" + item.id;
                                            }
                                            <a class="btn btn-danger mt-auto" href="@ProfilSil">Sil</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }

                    <div class="col-sm-3">
                        <div class="single-profile profile-no-border">
                            <div class="profile-txt">
                                <a href=""><i class="flaticon-github-logo"></i></a>
                                <div class="profile-icon-name">github</div>
                            </div>
                            <div class="single-profile-overlay">
                                <div class="profile-txt">
                                    <a href=""><i class="flaticon-github-logo"></i></a>
                                    <div class="profile-icon-name">github</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>

</section>
