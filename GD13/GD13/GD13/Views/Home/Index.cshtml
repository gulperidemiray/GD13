
@model (List<DTO.Reponse.GD13> Education, List<DTO.Reponse.profiles> ProfilListe, DTO.Reponse.GD13 Kullanici1)
@{
    ViewData["Title"] = "Gülperi Demiray";
}



<section id="welcome-hero" class="welcome-hero">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <div class="header-text">
                    <h2>Merhaba! Ben Gülperi DEMİRAY</h2>
                    <p>Bilgisayar Mühendisi</p>
                    <!-- Giriş Yap butonu -->
                    <a href="Oturum/Index" id="girisYapButton" class="btn btn-primary">Giriş Yap</a>
                </div>
            </div>
        </div>
    </div>
</section>

<section id="about" class="about">
    <div class="section-heading text-center">
        <h2>HAKKIMDA</h2>
    </div>
    <div class="container">
        <div class="about-content">
            <div class="row">
                <div class="col-sm-6">
                    <div class="single-about-txt">
                        <h3>Bilgisayar mühendisliği 3.sınıf öğrencisiyim.</h3>
                        <p>Öğrenime meraklı, her gün adım adım kendini geliştirmeye adamış bir gencim. Yaptıklarım yapmak isteyeceklerimden daha büyük.</p>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>

<section id="education" class="education">
    <div class="section-heading text-center">
        <h2>EĞİTİM</h2>
    </div>
    <div class="container">
        <div class="education-horizontal-timeline">
            <div class="row">
                @{
                    @foreach (var item in Model.Education)
                    {
                        <div class="col-sm-4">
                            <div class="single-horizontal-timeline">
                                <div class="experience-time">
                                    <h3>@item.Degree</h3>
                                </div><!--/.experience-time-->
                                <div class="timeline-horizontal-border">
                                    <i class="fa fa-circle" aria-hidden="true"></i>
                                    <span class="single-timeline-horizontal"></span>
                                </div>
                                <div class="timeline">
                                    <div class="timeline-content">
                                        <h4 class="title">@item.Institution</h4>
                                        <h5>@item.Location</h5>
                                        <p class="description">@item.Description</p>
                                    </div><!--/.timeline-content-->
                                </div><!--/.timeline-->
                            </div>
                        </div>
                    }}
            </div>
        </div>
    </div>
</section>

